{
    "name": "fieroo/payment",
    "description": "Fieroo Payment",
    "type": "library",
    "license": "EULA",
    "require": {
        "league/omnipay": "^3",
        "omnipay/paypal": "^3.0",
        "laravel/cashier": "^13.17"
    },
    "autoload": {
        "psr-4": {
            "Fieroo\\Payment\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "Francesco Passanante",
            "email": "francescopassanante@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Fieroo\\Payment\\Providers\\PaymentProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}